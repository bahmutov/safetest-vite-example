{
  "name": "vite-react-ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "report:dev": "vite --config vite.report.config",
    "report:build": "npx tsc --project tsconfig.report.json && vite build --config vite.report.config",
    "build": "npm i ../../ && (rm -rf ../../build/vite-react-ts || true) && vite build && mv dist ../../build/vite-react-ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.6",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "prettier": "^3.2.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-singlefile": "^0.13.5",
    "vitest": "^0.34.3"
  }
}
